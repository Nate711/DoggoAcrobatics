<!-- Cheetah Model

    The state space is populated with joints in the order that they are
    defined in this file. The actuators also operate on joints.

    State-Space (name/joint/parameter):
        - rootx     slider      position (m)
        - rootz     slider      position (m)
        - rooty     hinge       angle (rad)
        - bthigh    hinge       angle (rad)
        - bshin     hinge       angle (rad)
        - bfoot     hinge       angle (rad)
        - fthigh    hinge       angle (rad)
        - fshin     hinge       angle (rad)
        - ffoot     hinge       angle (rad)
        - rootx     slider      velocity (m/s)
        - rootz     slider      velocity (m/s)
        - rooty     hinge       angular velocity (rad/s)
        - bthigh    hinge       angular velocity (rad/s)
        - bshin     hinge       angular velocity (rad/s)
        - bfoot     hinge       angular velocity (rad/s)
        - fthigh    hinge       angular velocity (rad/s)
        - fshin     hinge       angular velocity (rad/s)
        - ffoot     hinge       angular velocity (rad/s)

    Actuators (name/actuator/parameter):
        - bthigh    hinge       torque (N m)
        - bshin     hinge       torque (N m)
        - bfoot     hinge       torque (N m)
        - fthigh    hinge       torque (N m)
        - fshin     hinge       torque (N m)
        - ffoot     hinge       torque (N m)

-->
<mujoco model="cheetah">
  <compiler angle="radian" coordinate="local" inertiafromgeom="true" settotalmass="4.8"/>

  <default>
    <joint armature=".1" damping="0.01" limited="true" solimplimit="0 .8 .03" solreflimit=".02 1" stiffness="0"/>
    <geom conaffinity="0" condim="3" contype="1" friction=".8 .1 .1" rgba="0.8 0.6 .4 1" solimp="0.0 0.8 0.01" solref="0.02 1"/>
    <motor ctrllimited="true" ctrlrange="-10 10" forcelimited="true" forcerange="-10 10"/>
  </default>



  <size nstack="300000" nuser_geom="1"/>
  <option gravity="0 0 -9.81" timestep="0.01"/>
  <!-- <option gravity="0 0 -0.1" timestep="0.01"/> -->
  <asset>
    <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
    <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
    <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
    <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
    <material name="geom" texture="texgeom" texuniform="true"/>
  </asset>


  <worldbody>
    <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
    <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>
    

    <body name="torso" pos="0 0 0.3">
      <camera name="track" mode="trackcom" pos="0 -2 0.3" xyaxes="1 0 0 0 0 1"/>
      <joint armature="0" axis="1 0 0" damping="0" limited="false" name="rootx" pos="0 0 0" stiffness="0" type="slide"/>
      <joint armature="0" axis="0 0 1" damping="0" limited="false" name="rootz" pos="0 0 0" stiffness="0" type="slide"/>
      <joint armature="0" axis="0 1 0" damping="0" limited="false" name="rooty" pos="0 0 0" stiffness="0" type="hinge"/>
      <!-- <joint armature="0" axis="0 1 0" damping="0" limited="false" name="rooty" pos="0 0 0" stiffness="0" type="free"/> -->

      <geom pos="0 0 0" name="torso" size="0.21 0.08 0.064" type="box"/>
      <!-- <site name='tip'  pos='.15 0 .11'/>-->
      

      <body name="bthigh" pos="-0.175 -0.1 0">
        <joint axis="0 1 0" name="bthigh" pos="0 0 0" range="-3 3" type="hinge"/>
        <geom axisangle="0 1 0 1.57" name="bthigh" pos="-0.045 0 0" size="0.015 .045" type="capsule" rgba = "1 0 0 1"/>

        <body name="bshin" pos="-0.09 0 ">
          <joint axis="0 1 0" name="bshin" pos="0 0 0" range="-0.7 1.8" type="hinge"/>
          <geom axisangle="0 1 0 -0.597" name="bshin" pos="0.045 0 -.066" rgba="1 0 1 1" size="0.015 .08" type="capsule"/>
        </body>
      </body>

      <body name="bthigh2" pos="-0.175 -0.1 0">
        <joint axis="0 1 0" name="bthigh2" pos="0 0 0" range="-3 3" type="hinge"/>
        <geom axisangle="0 1 0 1.57" name="bthigh2" pos="0.045 0 0" size="0.015 .045" rgba = "0 0 1 1" type="capsule"/>

        <body name="bshin2" pos="0.09 0 ">
          <joint axis="0 1 0" name="bshin2" pos="0 0 0" range="-3 3" type="hinge"/>
          <geom axisangle="0 1 0 0.597" name="bshin2" pos="-0.045 0 -.066" rgba="0 1 1 1" size="0.015 .08" type="capsule"/>
        </body>
      </body>

      <body name="fthigh" pos="0.175 -0.1 0">
        <joint axis="0 1 0" name="fthigh" pos="0 0 0" range="-3 3" type="hinge"/>
        <geom axisangle="0 1 0 1.57" name="fthigh" pos="-0.045 0 0" size="0.015 .045" type="capsule" rgba = "1 0 0 1"/>

        <body name="fshin" pos="-0.09 0 ">
          <joint axis="0 1 0" name="fshin" pos="0 0 0" range="-0.7 1.8" type="hinge"/>
          <geom axisangle="0 1 0 -0.597" name="fshin" pos="0.045 0 -.066" rgba="1 0 1 1" size="0.015 .08" type="capsule"/>
        </body>
      </body>

      <body name="fthigh2" pos="0.175 -0.1 0">
        <joint axis="0 1 0" name="fthigh2" pos="0 0 0" range="-3 3" type="hinge"/>
        <geom axisangle="0 1 0 1.57" name="fthigh2" pos="0.045 0 0" size="0.015 .045" rgba = "0 0 1 1" type="capsule"/>

        <body name="fshin2" pos="0.09 0 ">
          <joint axis="0 1 0" name="fshin2" pos="0 0 0" range="-3 3" type="hinge"/>
          <geom axisangle="0 1 0 0.597" name="fshin2" pos="-0.045 0 -.066" rgba="0 1 1 1" size="0.015 .08" type="capsule"/>
        </body>
      </body>

  
    </body>
  </worldbody>

  <equality>
    <connect name="bfoot" body1="bshin" body2="bshin2" anchor="0.09 0 -0.1323"/>
    <connect name="ffoot" body1="fshin" body2="fshin2" anchor="0.09 0 -0.1323"/>
  </equality>

  <actuator>
    <motor gear="1" joint="bthigh" name="bthigh"/>
    <motor gear="1" joint="bthigh2" name="bthigh2"/>
    <motor gear="1" joint="fthigh" name="fthigh"/>
    <motor gear="1" joint="fthigh2" name="fthigh2"/>
  </actuator>
</mujoco>
